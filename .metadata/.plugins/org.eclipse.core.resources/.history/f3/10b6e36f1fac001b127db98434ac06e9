<?php
//キーname を指定して配列に名前を格納
    $profile["name"] = "神田ゴロウ";

//キー身長を指定して配列に身長を格納

    $profile["身長"] = 169.5;
    
 //キーageを指定して配列に年齢を格納
 
    $profile["型"] = "A";
    
//キー型を指定して配列に血液型を格納 


    $profile["型"] = "A";
    
    
 echo "$profile[name] さんの身長は $profile[身長] cmです。<br>";
 
 echo "年齢は $profile[age] 歳、血液型は $profile[型] です。";
 
 ?>